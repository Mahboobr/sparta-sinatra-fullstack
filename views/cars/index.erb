
<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3" class="all_cars">
      <h1>Welcome to your garage</h1>
    </div>
  </div>
</div>

<% @cars.each do |car| %>
  <div class="container">
    <div class="row">
      <div class="col-md-3 col-md-offset-4" class="all_cars">
        <h3><a href="/<%= car.id %>"><%= car.manufacturer %>  <%= car.model %></a></h3>
      </div>
    </div>
  </div>
<% end %>
